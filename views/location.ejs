<div class="container">
   <h1>Location Info</h1>
    <p><%= dName %> としてログインしています</p>

   <% if (err){ %>
    <p><font color="red"><%= err %></font></p>
   <% }else{ %>
    <% for(var i=0; i<geodata.length; i++) { %>
  <div id="lat<%= i %>" title="<%= lat[i] %>"></div>
  <div id="lng<%= i %>" title="<%= lng[i] %>"></div>
    <% } %>
  <div id="num" title="<%= geodata.length %>"></div>
  <div id="map_canvas" style="width:640px; height:480px;"></div>
  <% } %>
</div>
<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true&language=ja"></script>
